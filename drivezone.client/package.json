{
  "name": "drivezone-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "DriveZone Premium Vehicle Rental Platform - Client Application",
  "keywords": [
    "vehicle-rental",
    "car-rental",
    "react",
    "vite",
    "typescript",
    "bootstrap",
    "drivezone"
  ],
  "author": "DriveZone Platform",
  "license": "MIT",
  "homepage": "https://drivezone.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/drivezone/drivezone-client.git"
  },
  "bugs": {
    "url": "https://github.com/drivezone/drivezone-client/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t drivezone-client .",
    "docker:run": "docker run -p 3000:80 drivezone-client"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "react-bootstrap": "^2.9.1",
    "react-icons": "^4.12.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "js-cookie": "^3.0.5",
    "date-fns": "^2.30.0",
    "react-datepicker": "^4.24.0",
    "react-select": "^5.8.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "lodash": "^4.17.21",
    "classnames": "^2.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "@types/testing-library__jest-dom": "^5.14.9",
    "sass": "^1.69.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^9.3.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "cssnano": "^6.0.1",
    "tailwindcss": "^3.3.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "babel-jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.4.3",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "identity-obj-proxy": "^3.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "cross-env": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
